<h1>Edit Contact</h1>
  <%= form_tag "/contacts/#{@contact.id}" , method: "patch" do %>
   <div>
  
   <div class="form-group">
     first_name: <input type="text" class="form-control" value = "<%= @contact.first_name %>" name="first_name" >
    </div>
   <div>
     
   <div class="form-group">
     last_name: <input type="text" class="form-control" value = "<%= @contact.last_name %>" name="last_name">
    </div>
   <div>
     
   <div class="form-group">
     email: <input type="email" class="form-control" value = "<%= @contact.email %>" name="email">
    </div>
   <div>
    
   <div class="form-group">
     phone_number: <input type="text" class="form-control" value = "<%= @contact.phone_number %>" name="phone_number">
    </div>
   
   <input type="submit" class="btn btn-default" value="Update Contact">
  <% end %>

<br>
<br>
<a href="/contacts">Back to the Home page</a>
</div>